.Login .login_Bg {   
    background-color: #f0f0f0!important;
    /* background-color: #1ea9ff!important; */
    background-image: none;
}
.Login .login_img {
    background: url(../public/assets/img/loginbg.png);
    background-position: bottom;
    background-repeat: no-repeat;
    background-size: contain;
    height: 100%;
    width: 100%;
}
.Login .login_logo {
    width: 180px;
    height: 180px;
    margin-top: 20px;
    transition: 2s ease-in-out;
    animation: wave;
    animation-iteration-count: infinite;
    animation-duration: 2.5s;
}
@keyframes wave {
    0%{
        transform: translate(0px, 0px);
    }   
    50%{
        transform: translate(0px, 20px);
    }
    100%{
        transform: translate(0px, 0px);
    }
    
}

.Login .card {
    width: 400px;
    box-shadow: 1px 1px 17px #6868681a;
    margin-left: 40px;
    border-radius: 40px!important;
}
.Login .card-body {
    padding: 20px 28px;
}

.Login form .form-group .label {
    position: absolute;
    transform: translate(20px, -35px);
    transition: 0.2s ease-in-out;
}
.Login form .form-group input:focus ~ .label span:nth-child(1) {
    animation: wavy1 .5s linear;    
}
.Login form .form-group input:focus ~ .label span:nth-child(2) {
    animation: wavy2 .5s linear;
    animation-delay: .1s;    
}
.Login form .form-group input:focus ~ .label span:nth-child(3) {
    animation: wavy3 .5s linear;
    animation-delay: .2s;    
}
.Login form .form-group input:focus ~ .label span:nth-child(4) {
    animation: wavy4 .5s linear;
    animation-delay: .3s;    
}
.Login form .form-group input:focus ~ .label span:nth-child(5) {
    animation: wavy5 .5s linear;
    animation-delay: .4s;    
}
.Login form .form-group input:focus ~ .label span:nth-child(6) {
    animation: wavy6 .5s linear;
    animation-delay: .5s;    
}
.Login form .form-group input:focus ~ .label span:nth-child(7) {
    animation: wavy7 .5s linear;
    animation-delay: .6s;    
}
.Login form .form-group input:focus ~ .label span:nth-child(8) {
    animation: wavy8 .5s linear;
    animation-delay: .7s;    
}

.custom-control-input:checked~.custom-control-label::before {
    color: #fff;
    border-color: #23d461;
    background-color: #23d461;
}

.Login form .form-group input:focus ~ .label, 
.Login form .form-group input:valid ~ .label
{

    transform: translate(15px, -61px);
    background: #23d461;
    color: #fff;
    box-shadow: 0px 0px 13px #ececec;
    border-radius: 17px;
    padding: 1px 10px;   
    font-size: 14px;    
    width: 79px;
    display: flex;
    justify-content: center;
    align-items: center;    
}

