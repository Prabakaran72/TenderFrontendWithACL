.Login form .login-btn {
    background: #007e2c;
    color: #fff;
    padding: 10px;
    width: 100%;
    border-radius: 40px;    
}
.login-btn.disabled, .login-btn:disabled {
    color: #fff;
    background-color: #12c350;
    border-color: #12c350;
}

.Login form  button .dfx{
    display: flex;
    justify-content: center;
    align-items: center;
    
}
.Login form  button .dfx .fa-gavel{
    position: absolute;
    bottom: 56px;
    left: 53px;
    font-size: 27px;
    transition: .5s ease-in;
}
.Login form  button:hover .dfx .fa-gavel{
    transform: rotate(45deg);
    transition: .1s ease-out;
}
.Login form  button .dfx .fa-gavel.hammer {
    position: absolute;
    bottom: 56px;
    left: 53px;
    font-size: 27px;
    animation: hammer .4s infinite;
    transition: .5s ease-in;
}
@keyframes hammer {
    0%{
        transform: rotate(0deg);        
    }
    100%{
        transform: rotate(45deg);
        transition: .1s ease-out!important;
    }
}


.Login form p.text-danger{
    padding-bottom: 11px;
}
.Login form p.text-danger span{
    position: absolute;
    bottom: 14px;
    left: 100px;
}

.Login form .form-group .form-control-user {
    margin-top: 25px;
}

.form-control:focus {
    border-color: #23d461!important;
    box-shadow: unset!important;
}
.btn-primary.disabled, .btn-primary:disabled {
    color: #fff;
    background-color: #12c350;
    border-color: #12c350;
}
.btn-primary {
    color: #fff;
    background-color: #12c350;
    border-color: #12c350;
}
.btn-primary:hover {
    color: #fff;
    background-color: #12c350;
    border-color: #12c350;
    box-shadow: 1px 1px 13px #ebebeb;
}
